{
  "version": 2,
  "regions": [
    "all"
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "APP_MODE": "live",
    "ENCRYPTION_IV": "@encryption_iv",
    "ENCRYPTION_KEY": "@encryption_key",
    "MAILER_API_KEY": "@mailer_api_key",
    "DB_HOST": "@db_host",
    "DB_PORT": "@db_port",
    "DB_USERNAME": "@db_username",
    "DB_PASSWORD": "@db_password",
    "TRACE_CLOUD_ENDPOINT": "@trace_cloud_endpoint",
    "PADDLE_AUTH_CODE": "@paddle_auth_code"
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "APP_MODE": "live",
      "NPM_TOKEN": "@npm_token",
      "ENCRYPTION_IV": "@encryption_iv",
      "ENCRYPTION_KEY": "@encryption_key",
      "MAILER_API_KEY": "@mailer_api_key",
      "DB_HOST": "@db_host",
      "DB_PORT": "@db_port",
      "DB_USERNAME": "@db_username",
      "DB_PASSWORD": "@db_password",
      "TRACE_CLOUD_ENDPOINT": "@trace_cloud_endpoint",
      "PADDLE_AUTH_CODE": "@paddle_auth_code"
    }
  },
  "routes": [
    {
      "src": "/await-email",
      "dest": "/"
    },
    {
      "src": "/sign-up-done",
      "dest": "/"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
